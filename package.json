{
  "name": "abschlussprojekt-reiseinfo-syrien-deutschland",
  "version": "1.0.0",
  "description": "Reise-Website fÃ¼r Syrien-Deutschland mit Backend API und React Frontend",
  "private": true,
  "type": "module",
  "workspaces": ["backend", "frontend"],
  "scripts": {
    "dev": "bun run --parallel dev:backend dev:frontend",
    "dev:backend": "cd backend && bun index-bun.ts",
    "dev:frontend": "cd frontend && bun run dev",
    "build": "bun run build:backend && bun run build:frontend",
    "build:backend": "cd backend && bun run build",
    "build:frontend": "cd frontend && bun run build",
    "start": "cd backend && bun run start",
    "prisma:generate": "cd backend && bun prisma generate",
    "prisma:push": "cd backend && bun prisma db push",
    "prisma:seed": "cd backend && node prisma/seed.js",
    "prisma:studio": "cd backend && bun prisma studio",
    "install:all": "bun install && cd backend && bun install && cd ../frontend && bun install"
  },
  "devDependencies": {
    "bun": "^1.3.0"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Maik-Protze/abschlussprojekt-reiseinfo-Syrien-Deutschland.git"
  },
  "author": "Maik Protze",
  "license": "MIT"
}